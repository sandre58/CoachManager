<UserControl x:Class="My.CoachManager.Presentation.Prism.Modules.Roster.Views.PlayerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModels1="clr-namespace:My.CoachManager.Presentation.Prism.Modules.Roster.ViewModels"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=viewModels1:PlayerViewModel, IsDesignTimeCreatable=True}"
             d:DesignHeight="500" d:DesignWidth="800">
    <Grid>
        <TextBlock Text="{Binding Item.FullName}" />
    </Grid>
</UserControl>