<views:WorkspaceView
        x:Class="My.CoachManager.Presentation.Views.Home.HomeView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:micro="http://www.caliburnproject.org"
        xmlns:controls="clr-namespace:My.CoachManager.Presentation.Controls"
        xmlns:viewModels="clr-namespace:My.CoachManager.Presentation.ViewModels.Home"
        xmlns:views="clr-namespace:My.CoachManager.Presentation.Controls.Views"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type={x:Type viewModels:HomeViewModel}}"
        micro:Bind.AtDesignTime="True"
        d:DesignHeight="300" d:DesignWidth="300">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBlock Text="Home" />
    </Grid>
</views:WorkspaceView>