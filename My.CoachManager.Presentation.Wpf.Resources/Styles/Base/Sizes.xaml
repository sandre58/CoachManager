<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:Double x:Key="VerySmallHeight">5</sys:Double>
    <sys:Double x:Key="VerySmallWidth">20</sys:Double>

    <sys:Double x:Key="SmallHeight">15</sys:Double>
    <sys:Double x:Key="SmallWidth">50</sys:Double>

    <sys:Double x:Key="MidSmallHeight">20</sys:Double>
    <sys:Double x:Key="MidSmallWidth">100</sys:Double>

    <sys:Double x:Key="MediumHeight">25</sys:Double>
    <sys:Double x:Key="MediumWidth">150</sys:Double>

    <sys:Double x:Key="SmallLargeHeight">30</sys:Double>
    <sys:Double x:Key="SmallLargeWidth">200</sys:Double>

    <sys:Double x:Key="LargeHeight">45</sys:Double>
    <sys:Double x:Key="LargeWidth">300</sys:Double>

    <sys:Double x:Key="VeryLargeHeight">60</sys:Double>
    <sys:Double x:Key="VeryLargeWidth">450</sys:Double>

    <sys:Double x:Key="VeryVeryLargeHeight">75</sys:Double>
    <sys:Double x:Key="VeryVeryLargeWidth">600</sys:Double>

    <!-- Diameters -->
    <sys:Double x:Key="VerySmallDiameter">15</sys:Double>
    <sys:Double x:Key="SmallDiameter">20</sys:Double>
    <sys:Double x:Key="NormalDiameter">30</sys:Double>
    <sys:Double x:Key="LargeDiameter">35</sys:Double>
    <sys:Double x:Key="VeryLargeDiameter">42</sys:Double>
</ResourceDictionary>